<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="true"/>
		<setting name="lazyLoadingEnabled" value="false"/>
		<setting name="multipleResultSetsEnabled" value="true"/>
		<setting name="useColumnLabel" value="true"/>
		<setting name="useGeneratedKeys" value="false"/>
		<setting name="autoMappingBehavior" value="PARTIAL"/>
		<setting name="defaultExecutorType" value="SIMPLE"/>
		<setting name="defaultStatementTimeout" value="25"/>
		<setting name="safeRowBoundsEnabled" value="false"/>
		<setting name="mapUnderscoreToCamelCase" value="false"/>
		<setting name="localCacheScope" value="SESSION"/>
		<setting name="jdbcTypeForNull" value="OTHER"/>
		<setting name="lazyLoadTriggerMethods" value="equals,clone,hashCode,toString"/>
	</settings>
    <mappers>
        <mapper resource="com/yang/lolvideo/dao/mapper/VideoTypeDaoMapper.xml"/>
        <mapper resource="com/yang/lolvideo/dao/mapper/HeroDaoMapper.xml"/>
        <mapper resource="com/yang/lolvideo/dao/mapper/UserLogDaoMapper.xml"/>
        <mapper resource="com/yang/lolvideo/dao/mapper/AnnouncerDaoMapper.xml"/>
        <mapper resource="com/yang/lolvideo/dao/mapper/BulletinDaoMapper.xml"/>
        <mapper resource="com/yang/lolvideo/dao/mapper/CatalogDaoMapper.xml"/>
        <mapper resource="com/yang/lolvideo/dao/mapper/VideoDaoMapper.xml"/>
        <mapper resource="com/yang/lolvideo/dao/mapper/VersionDaoMapper.xml"/>
        <mapper resource="com/yang/lolvideo/dao/mapper/EpisodeDaoMapper.xml"/>
        <mapper resource="com/yang/lolvideo/dao/mapper/UserKeywordDaoMapper.xml"/>
        
    </mappers>
    
</configuration>